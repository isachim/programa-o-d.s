class Conta:

def _init_(self):
    print("Construindo objeto...{}".format(self))
    self.numero = numero
    self.titular = titular
    self.saldo = saldo 
    self.limite = limite

def extrato (self):
    print( "saldo de {} do titular {}".format(self.saldo, self.titular))

def deposita(self, valor):
    self.saldo += valor

def saca (self, valor):
    self.saldo -= valor
